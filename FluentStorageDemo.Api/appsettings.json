{
  "Aws": {
    "AccessKey": "${Aws:AccessKey}",
    "SecretKey": "${Aws:SecretKey}",
    "Region": "us-east-1"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BlobStorage": {
    "Provider": "s3",
    "S3BucketName": "fluent-storage-demo"
  }
}
